{
  "expo": {
    "name": "pet-app",
    "slug": "pet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-@2x.png",    
    "iconBackgroundColor": "#FFFEFC",
    
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFEFC"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-@2x.png",
          "imageWidth": 300,
          "resizeMode": "native",
          "backgroundColor": "#FFFEFC"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
