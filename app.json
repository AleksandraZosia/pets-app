{
  "expo": {
    "name": "pet-app",
    "slug": "pet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-@2x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFEFC"
      },
      "package": "com.aleksandrazosia.petapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-@2x.png",
          "imageWidth": 300,
          "resizeMode": "native",
          "backgroundColor": "#FFFEFC"
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "df96178f-d9a8-4f78-b2e6-55803fbeb201"
      }
    },
    "owner": "aleksandrazosia"
  }
}
